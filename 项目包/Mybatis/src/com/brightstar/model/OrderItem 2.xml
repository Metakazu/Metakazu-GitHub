<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 命名空间 -->
<mapper namespace="com.brightstar.model">
	<insert id="insertOrderItem" parameterType="Map">
		insert into datebase.order_ITEM values(null,#{oid},#{pid},#{number});
	</insert>

	<delete id="deleteOrderItem" parameterType="Map">
		delete from datebase.order_item where oid=#{oid} and pid=#{pid};
</delete>

</mapper>